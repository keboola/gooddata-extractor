{
  "type": "object",
  "title": "Parameters",
  "required": [
    "writer_id",
    "pid",
    "username",
    "#password",
    "reports",
    "host"
  ],
  "properties": {
    "pid": {
      "type": "string",
      "title": "Provisioned GoodData Project Id (deprecated, ignored)",
      "propertyOrder": 5000
    },
    "host": {
      "type": "string",
      "title": "GoodData Host",
      "default": "secure.gooddata.com",
      "description": "secure.gooddata.com, keboola.eu.gooddata.com, or a custom host",
      "propertyOrder": 1000
    },
    "reports": {
      "type": "array",
      "items": {
        "type": "string",
        "title": "Uri"
      },
      "title": "Reports",
      "format": "tab",
      "description": "e.g. /gdc/md/wasc4gjy5sphvlt0wjx5fqys5q6bh38j/obj/2284",
      "propertyOrder": 2000
    },
    "username": {
      "type": "string",
      "title": "GoodData Username",
      "default": "",
      "propertyOrder": 3000
    },
    "#password": {
      "type": "string",
      "title": "GoodData Password",
      "format": "password",
      "propertyOrder": 4000
    },
    "writer_id": {
      "type": "string",
      "title": "GoodData Writer ID (deprecated, ignored)",
      "propertyOrder": 6000
    }
  }
}
