{
  "name": "keboola/gooddata-extractor",
  "description": "Keboola GoodData Extractor for KBC docker",
  "keywords": ["keboola", "gooddata", "extractor"],
  "authors": [
    {
      "name": "Jakub Matejka",
      "email": "jakub@keboola.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.2",
    "keboola/csv": "~1.1.3",
    "keboola/gooddata-php-client": "^3.0",
    "keboola/storage-api-client": "^10.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Keboola\\": "src/Keboola/"
    }
  },
  "scripts": {
    "test": "phpunit --bootstrap tests/bootstrap.php --process-isolation --fail-on-warning --stop-on-failure --testsuite main tests/Keboola/GoodDataExtractor",
    "phpcs": "phpcs --standard=psr2 -n --ignore=vendor --extensions=php .",
    "build": [
      "@phpcs",
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
