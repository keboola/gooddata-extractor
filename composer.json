{
  "name": "keboola/gooddata-extractor",
  "description": "Keboola GoodData Extractor for KBC docker",
  "keywords": ["keboola", "gooddata", "extractor"],
  "authors": [
    {
      "name": "Jakub Matejka",
      "email": "jakub@keboola.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.2",
    "keboola/csv": "~1.1.3",
    "symfony/yaml": "~2.6.3",
    "keboola/php-temp": "0.1.3",
    "keboola/gooddata-php-client": "^2.2",
    "keboola/storage-api-client": "^10.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "squizlabs/php_codesniffer": "2.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Keboola\\": "src/Keboola/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpcs": "phpcs --standard=psr2 -n --ignore=vendor --extensions=php .",
    "build": [
      "@phpcs",
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
